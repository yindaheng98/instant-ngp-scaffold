{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "stnerf2nerf",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/stnerf2nerf.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--aabb_scale",
                "8",
                "--path",
                "data/nerf/taekwondo"
            ]
        },
        {
            "name": "parse_seq",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/parse_seq.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "args": [
                "--start",
                "1",
                "--end",
                "101",
                "--saveformat",
                "results/stnerf-taekwondo/frame%d.bson",
                "--intraexportformat",
                "results/stnerf-taekwondo/frame%d-intra.npz",
                "--interexportformat",
                "results/stnerf-taekwondo/frame%d-inter.npz",
                "--interdiffexportformat",
                "results/stnerf-taekwondo/frame%d-interdiff.npz"
            ]
        },
        {
            "name": "PSNRvsCompression",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/figures/PSNRvsCompression.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "args": [
                "--start",
                "1",
                "--end",
                "101",
                "--qualityformat",
                "results/stnerf-taekwondo/frame%d.bson.json",
                "--diffqualityformat",
                "results/stnerf-taekwondo/frame%(i)dT=%(T)fT_density=%(T_density)f-snapshotsimulate.bson.json",
                "--sizeformat",
                "results/stnerf-taekwondo/frame%d-intra.json",
                "--diffsizeformat",
                "results/stnerf-taekwondo/frame%(i)dT=%(T)f-inter.json",
            ]
        }
    ]
}