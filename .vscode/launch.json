{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "stnerf2nerf",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/stnerf2nerf.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--aabb_scale",
                "8",
                "--path",
                "data/nerf/taekwondo"
            ]
        },
        {
            "name": "run",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/run.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "args": [
                "--load_snapshot",
                "results/stnerf-walking/frame1.bson",
                "--gui"
            ]
        },
        {
            "name": "parse_seq",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/parse_seq.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "args": [
                "--start",
                "1",
                "--end",
                "101",
                "--saveformat",
                "results/stnerf-taekwondo/frame%d.bson",
                "--intraexportformat",
                "results/stnerf-taekwondo/frame%d-intra.npz",
                "--interexportformat",
                "results/stnerf-taekwondo/frame%d-inter.npz",
                "--interdiffexportformat",
                "results/stnerf-taekwondo/frame%d-interdiff.npz"
            ]
        },
        {
            "name": "parse_seq2bson",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/parse_seq2bson.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "args": [
                "--start",
                "1",
                "--end",
                "101",
                "--saveformat",
                "results/stnerf-taekwondo/frame%d.bson",
                "--intraexportformat",
                "results/stnerf-taekwondo/frame%d-intra.bson",
                "--interexportformat",
                "results/stnerf-taekwondo/frame%(i)dT=%(T)f-inter.bson",
                "-T",
                "0.0001"
            ]
        },
        {
            "name": "intra play mt",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/play_mt.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "args": [
                "--load_snapshot",
                "results/stnerf-taekwondo/frame1.bson",
                "--init",
                "results/stnerf-taekwondo/frame1-intra.bson",
                "--frameformat",
                "results/stnerf-taekwondo/frame%d-inter.bson",
                "--start",
                "2",
                "--end",
                "101"
            ]
        }
    ]
}