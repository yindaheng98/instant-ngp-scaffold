{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "stnerf2nerf",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/stnerf2nerf.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--aabb_scale",
                "8",
                "--path",
                "data/nerf/taekwondo"
            ]
        },
        {
            "name": "train_seq",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/train_seq.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "args": [
                "--steps",
                "10000",
                "--start",
                "1",
                "--end",
                "101",
                "--dataformat",
                "data/nerf/taekwondo/frame%d",
                "--saveformat",
                "results/stnerf-taekwondo/frame%d-nofreeze.bson",
                "--executable",
                "./instant-ngp-train",
                "--no_freeze"
            ]
        },
        {
            "name": "PSNRvsCompression",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/figures/PSNRvsCompression.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "args": [
                "--start",
                "1",
                "--end",
                "101",
                "--qualityformat",
                "results/stnerf-taekwondo/frame%d.bson.json",
                "--diffqualityformat",
                "results/stnerf-taekwondo/frame%(i)dT=%(T)fT_density=%(T_density)f-snapshotsimulate.bson.json",
                "--nofreezequalityformat",
                "results/stnerf-taekwondo/frame%(i)d-nofreezeT=%(T)fT_density=%(T_density)f-snapshotsimulate.bson.json",
                "--sizeformat",
                "results/stnerf-taekwondo/frame%d-intra.json",
                "--diffsizeformat",
                "results/stnerf-taekwondo/frame%(i)dT=%(T)f-inter.json",
                "--nofreezesizeformat",
                "results/stnerf-taekwondo/frame%(i)d-nofreezeT=%(T)f-inter.json",
            ]
        },
        {
            "name": "PSNRvsCompressionMorePoints",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/figures/PSNRvsCompressionMorePoints.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "args": [
                "--start",
                "1",
                "--end",
                "101",
                "--qualityformat",
                "results/stnerf-taekwondo/frame%d.bson.json",
                "--diffqualityformat",
                "results/stnerf-taekwondo/frame%(i)dT=%(T)fT_density=%(T_density)f-snapshotsimulate.bson.json",
                "--sizeformat",
                "results/stnerf-taekwondo/frame%d-intra.json",
                "--diffsizeformat",
                "results/stnerf-taekwondo/frame%(i)dT=%(T)f-inter.json",
                "--nofreezequalityformat",
                "results/stnerf-taekwondo/frame%(i)d-nofreezeT=%(T)fT_density=%(T_density)f-snapshotsimulate.bson.json",
                "--nofreezesizeformat",
                "results/stnerf-taekwondo/frame%(i)d-nofreezeT=%(T)f-inter.json",
            ]
        },
        {
            "name": "PSNRvsTime",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/figures/PSNRvsTime.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "args": [
                "--start",
                "1",
                "--end",
                "101",
                "--qualityformat",
                "results/stnerf-taekwondo/frame%d.bson.json",
                "--diffqualityformat",
                "results/stnerf-taekwondo/frame%(i)dT=%(T)fT_density=%(T_density)f-snapshotsimulate.bson.json",
                "--sizeformat",
                "results/stnerf-taekwondo/frame%d-intra.json",
                "--diffsizeformat",
                "results/stnerf-taekwondo/frame%(i)dT=%(T)f-inter.json",
                "--nofreezequalityformat",
                "results/stnerf-taekwondo/frame%(i)d-nofreezeT=%(T)fT_density=%(T_density)f-snapshotsimulate.bson.json",
                "--nofreezesizeformat",
                "results/stnerf-taekwondo/frame%(i)d-nofreezeT=%(T)f-inter.json",
            ]
        }
    ]
}